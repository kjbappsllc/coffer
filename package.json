{
  "name": "coffer",
  "version": "0.0.2",
  "description": "Full fledged budget application.",
  "repository": {
    "github": "https://github.com/kjbappsllc/coffer"
  },
  "main": "index.js",
  "bin": "./build/proj-build/external-interfaces/cli/index.js",
  "scripts": {
    "start": "npm-run-all clean build:proj --parallel build:web:dev run:servers",
    "start:fresh": "npm-run-all clear-db start",
    "build:prod": "npm-run-all build:proj build:web",
    "build:proj": "babel ./src --out-dir ./build/proj-build",
    "build:web": "webpack --mode production",
    "build:web:dev": "webpack --mode development --watch --hot",
    "run:servers": "node ./build/proj-build",
    "clean": "del-cli build/proj-build/",
    "clear-db": "del-cli ./database/",
    "test": "mocha --require @babel/register ./src/**/*.test.js",
    "dply:test": "npm run build:prod && npm install -g . && coffer",
    "dply:patch": "npm run build:prod && npm version patch && npm install -g .",
    "dply:minor": "npm run build:prod && npm version minor && npm install -g .",
    "dply:major": "npm run build:prod && npm version major && npm install -g ."
  },
  "keywords": [
    "budget",
    "cli",
    "react"
  ],
  "author": "Keyshawn Butts",
  "email": "kbutts314@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "del-cli": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.5.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "chart.js": "^2.8.0",
    "hapi": "^17.8.5",
    "inert": "^5.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "nedb": "^1.8.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "rxjs": "^6.5.2",
    "sjcl": "^1.0.8"
  }
}
